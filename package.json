    "@nestjs/core": "^10.4.15"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.15",
    "@nestjs/core": "^10.4.15",
    "@nestjs/platform-express": "^10.0.0",
    "axios": "^1.7.9",
